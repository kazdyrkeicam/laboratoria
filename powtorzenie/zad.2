//NAUCZ SIE QSORTA!!!!

#include <string.h>
#include <stdio.h>
#include <stdlib.h>

int cmp_strings(const void *str1, const void *str2){
  return strcmp((char*)str1, (char*)str2);
}


int main(){
  char arr[5][100] = {"tygrys azjatycki", "tygrys syberyjski", "chomik", "slon", "kot"};
  
  qsort(arr, 5, 100, cmp_strings);
  
  for(int i = 0; i < 5; ++i)
    printf("%s\n", arr[i]);
  return 0;
}

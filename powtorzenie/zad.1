#include <stdio.h>

typedef enum {ADDITION, SUBTRACTION, MULTIPLICATION, DIVISION, UNKNOW} Operations;

Operations f(int a, int b, int result){
  if(a + b == result) return ADDTION;
  else if(a - b == result) return SUBTRACTION;
  else if(a * b == result) return MULTIPLICATION;
  else if(a / b == result) return DIVISION;
  else return UNKNOW;
}

int main(){
  printf("%d\n", f(2,2,4));
  return 0;
}

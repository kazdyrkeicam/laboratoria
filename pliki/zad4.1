#include <stdio.h>


int main(){
  FILE *file = NULL;
  
  if((file = fopen("test.bin", "r+b")) == NULL){    //w czytanie b binarny tryb
    perror("blad");
    return 1;
  } 
  else {
    int arr[10] = {0};
    
    fread(arr, sizeof(int), 10, file);
    for(int i = 0; i < 10; ++i)
      ++arr[i];
    fwrite(arr, sizeof(int), 10, file);
    fclose(file);
    
    if(rename("test", "test_new") == 0)
      printf("Wszystko ok!");
    else
      printf("nie wszystko ok");
  }
  return 0;
}
